//https://www.codechef.com/problems/DOLL

#include <iostream>
using namespace std;

int main() {
    int t;
	cin>>t;

	for(int i=0;i<t;i++){
	  int N,K;
	   cin>>N>>K;
	  int A[N];
	  int count=0;
	 
	  
	  for(int i=0;i<N;i++){
	  cin>>A[i];
	     if (A[i]>K){
	      count++; 
	  }
	}

	  
	 cout<<count<<endl;   
	}
	return 0;
}
