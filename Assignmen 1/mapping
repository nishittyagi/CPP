//https://www.ideone.com/FKQQuR

#include <bits/stdc++.h>
using namespace std;

int main() {
	int n,number;
	cin>>n;
	string name;
	unordered_map<string,int>marks;
	for(int i=0;i<n;i++){
		cin>>name>>number;
		marks[name]=number;
	}
	cout<<"marks of Nishit "<<marks["Nishit"]<<endl;
	cout<<"marks of Nitin "<<marks["Nitin"]<<endl;
	return 0;
}
