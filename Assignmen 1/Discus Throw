//https://www.codechef.com/problems/DISCUS

#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin>>t;
	for(int i=0;i<t;i++){
     int a,b,c;
     cin>>a>>b>>c;
     if(a>b && a>c)cout<<a<<endl;
     else if(b>a && b>c)cout<<b<<endl;
     else if(c>a && c>b)cout<<c<<endl;
     else if (a==b && a>c)cout<<a<<endl;
     else if (b==c && b>a)cout<<b<<endl;
     else if (a==c && c>b)cout<<c<<endl;
     else cout<<a<<endl;
	    
	}
	
	
	return 0;
}
